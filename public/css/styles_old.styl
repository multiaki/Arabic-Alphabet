    margin-top: 10px;
    margin-left: auto;
    clear: none;
    margin-right: auto;
    margin-bottom: 0px;
    font-style: italic;
    color: rgb(26, 119, 57);
    text-shadow: rgb(255, 255, 255) 0px 0px 1px;
    font-family: aw-conqueror-carved-one;
    text-decoration: none;
}

