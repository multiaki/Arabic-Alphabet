}

#text {
    float: none;
    font-size: 2.5em;
    width: auto;
    height: auto;
    text-align: center;
    font-weight: 900;
    line-height: normal;
    margin-left: auto;
    margin-top: 5px;
    clear: none;
    min-height: 0px;
    font-style: italic;
    color: rgb(255, 255, 255);
    text-shadow: rgb(26, 119, 57) 1px 1px 3px;
    font-family: aw-conqueror-carved-one;
    text-decoration: none;
    margin-right: auto;
}

@media only screen and (max-width: 1250px) {
    #main-wrapper {
        width: 100%;
        min-width: 0px;
        padding-top: 0px;
        padding-right: 0px;
        padding-bottom: 0px;
        padding-left: 0px;
    }

    #game-wrapper {
        clear: none;
        float: none;
        margin-left: auto;
        margin-right: auto;
    }

    #sidebar {
        margin-left: auto;
        margin-right: auto;
        float: none;
        clear: none;
        margin-top: 30px;
        width: 768px;
    }

    #content-wrapper {
        background-image:url("../img/registerpanel.png");
        background-attachment: scroll;
        background-size: contain;
        background-position: 0% 50px;
        background-repeat: no-repeat no-repeat;
    }
}
