/*
 * Import Nib
 */
@import 'nib'

/*
 * Nib Reset File
 */
global-reset()

/*
 * Set Variables
 */
main-color = #333333
background-color = #f4f4f4
border-color = rgba(104, 163, 179, 0.541176);

.primaryContainer {
    height: auto;
    margin-left: auto;
    margin-right: auto;
    min-height: 100%;
    width: 100%;
}

#game-wrapper {
    height: 1024px;
    width: 768px;
    margin: 0px;
	border: 1px solid rgba(104, 163, 179, 0.541176);
    border-radius: 10px;
   	position: relative;
    background-image:url("../img/bg.jpg");
    background-position: 0% 0%;
    background-repeat: repeat;
    background-attachment: scroll;
    background-size: auto;
	float:left;
}

#sidebar {
    float: right;
    height: 1024px;
    margin: 0px;
    clear: none;
    width: 30%;
    background: rgb(227, 249, 255);
    border: 1px solid rgba(104, 163, 179, 0.541176);
}

#main-wrapper {
    float: none;
    margin: 40px auto 0;
    clear: none;
    width: 1200px;
    max-width: 1200px;
    min-width: 768px;
    padding: 0px;
}

#content-wrapper {
    float: none;
    height: 786px;
    margin-left: auto;
    margin-top: 80px;
    clear: none;
    width: 616px;
    position: relative;
    top: 0px;
    left: 0px;
    margin-right: auto;
    background-image:url("../img/registerpanel.png");
    background-position: 0% 50px;
    background-repeat: no-repeat;
    background-attachment: scroll;
    background-size: auto;
    padding-top: 0px;
    margin-bottom: 0px;
}

#fulname-wrapper {
    float: none;
    height: 108.375px;
    display: block;
    text-align: left;
    margin-left: auto;
    margin-top: 25px;
    clear: none;
    width: 79.999493%;
    min-height: auto;
    background-image:url("../img/input-field.png");
    background-position: 0% 0%;
    background-repeat: no-repeat;
    background-attachment: scroll;
    background-size: auto;
    margin-right: auto;
}

#fullname-input {
    float: none;
    height: 36px;
    padding-top: 0px;
    padding-right: 10px;
    padding-bottom: 0px;
    padding-left: 10px;
    font-size: 1.5em;
    display: block;
    width: 89.998722%;
    background-color: rgba(255, 0, 0, 0);
    margin-top: 10px;
    margin-left: auto;
    clear: none;
    margin-right: auto;
    margin-bottom: 0px;
    font-family: aw-conqueror-carved-one;
    color: rgb(255, 255, 255);
}

#fulname-label {
    float: none;
    font-size: 1.5em;
    width: 90%;
    height: auto;
    text-align: left;
    font-weight: 900;
    line-height: normal;
    padding-top: 3px;
    padding-bottom: 3px;
    padding-left: 3px;
    padding-right: 3px;
    display: block;
